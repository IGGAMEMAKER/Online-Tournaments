mixin title(name)
	//div(class="bloc l-bloc bgc-white" id="bloc-1")
		div(class="container")
			div(class="row")
				div(class="col-sm-12")
					h2(class="mg-md text-center") #{name}
	h2(class="mg-md text-center") #{name}

mixin small_title(name)
	//div(class="bloc l-bloc bgc-white" id="bloc-1")
		div(class="container")
			div(class="row")
				div(class="col-sm-12")
					h3(class="mg-md text-center") #{name}
	h3(class="mg-md text-center") #{name}
